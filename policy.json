{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "CWLogs",
            "Effect": "Allow",
            "Principal": {
                "Service": "logs.amazonaws.com"
            },
            "Action": "s3:PutObject",
            "Resource": "arn:aws:s3:::70491-log-bucket/*"
        },
        {
            "Sid": "CWLogsAcl",
            "Effect": "Allow",
            "Principal": {
                "Service": "logs.amazonaws.com"
            },
            "Action": "s3:GetBucketAcl",
            "Resource": "arn:aws:s3:::70491-log-bucket"
        }
    ]
}